	width: 40%;
}

.twoItems .leftNav,
.twoItems .rightNav,
.twoItems .centerNav {
	width: 8%;
}
.twoItems .centerNav {
	width: 16%;
}
.twoItems .secNavLink {
	width: 34%;
}


.secList {
	background-color: #ffa6a6;
	background-image: url("../images/secundaryNav_bg.png");
}

.secundaryNav a {
	color: #555555;
	display: block;
	font-size: 0.75em;
	height: 16px;
	line-height: 16px;
	margin: 0 16px;
	text-transform: uppercase;
}

	.upLink a {
		background: url('../images/secundaryNav_arrows.png') left top no-repeat,
					url('../images/secundaryNav_arrows.png') right top no-repeat;
	}

	.upLink a:hover {
		background: url('../images/secundaryNav_arrows.png') left -32px no-repeat,
					url('../images/secundaryNav_arrows.png') right -32px no-repeat;
	}
	
	.downLink a {
		background: url('../images/secundaryNav_arrows.png') left -64px no-repeat,
					url('../images/secundaryNav_arrows.png') right -64px no-repeat;
	}
	
	.downLink a:hover {
		background: url('../images/secundaryNav_arrows.png') left bottom no-repeat,
					url('../images/secundaryNav_arrows.png') right bottom no-repeat;
	}

