/*** IE FIX ***/

/* Pop Up */

.noOverflow {
	overflow: hidden;
}

#changeBrowser {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: #fff;
	z-index: 1000;
	display: none;
}

	#changeBrowser h2,
	#changeBrowser h3 {
		margin-bottom: 16px;
	}
	
	#wrapperPopUp {
		width: 34em;
		padding-bottom: 0.5em;
		background: url('../images/popUpB.png') bottom center repeat-x;
		position: absolute;
		left: 50%;
		top: 0;
		margin-left: -18em;
	}

		#cbPopUp {
			padding: 1em;
			background: url('../images/body_bg.jpg');
			text-align: center;
		}
		
		#cbPopUp h2 {
			font-size: 3em;
			color: #ff6300;
		}
		
		#cbPopUp h3 {
			font-size: 2em;
			color: #D2A432;
		}
		
		#cbPopUp p {
			font-size: 1em;
			color: #333;
		}
		
			#cbPopUp p strong {
				font-weight: bold;
				color: #000;
			}
	
.lastBrowsers {
	margin: 2em 0;
}
	
	.lastBrowsers li {
		display: inline-block;
		margin: 0 0.5em;
	}
	
		.lastBrowsers li a {
			display: block;
			border: 0;
			margin-bottom: 1em;
		}
		
			.lastBrowsers li a img {
				display: block;
